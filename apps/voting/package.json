        "jotai": "^2.5.1",
        "jotai-effect": "^0.2.3",
        "react": "^18.2.0",
        "react-bootstrap": "^2.9.1",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@concordium/ccd-js-gen": "^1.0.1",
