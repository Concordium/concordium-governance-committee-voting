    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build-election-contract": "cargo concordium build --schema-embed --out ../../contracts/concordium-governance-committee-election/concordium-out/module.wasm.v1 -- --manifest-path ../../contracts/concordium-governance-committee-election/Cargo.toml",
